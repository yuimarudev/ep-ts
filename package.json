{
  "name": "ts-empower",
  "version": "1.0.0",
  "description": "Rust saikyo!",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest",
    "build": "find dist/* | egrep -v 'package.json' | xargs rm -rf && node esbuild.config.js && tsc",
    "lint": "prettier --check src/**/*.ts && eslint --fix --ext .ts src",
    "format": "prettier --write src/**/*.ts"
  },
  "author": "yuimarudev",
  "license": "MIT",
  "devDependencies": {
    "@swc-node/register": "^1.6.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "ts-empower": "file:dist",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "type": "module",
  "pnpm": {
    "patchedDependencies": {
      "@swc-node/register@1.6.7": "patches/@swc-node__register@1.6.7.patch"
    }
  },
  "packageManager": "pnpm@8",
  "dependencies": {
    "@types/node": "^20.5.8"
  }
